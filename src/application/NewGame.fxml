<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600" prefWidth="600" style="-fx-background-color: #1E1F26;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.NewGameController">
	<children>
		<Label layoutX="218.0" layoutY="43.0" text="New Game" textFill="#d0e1f9">
			<font>
				<Font name="Krungthep" size="30.0" />
			</font>
		</Label>
		<Button layoutX="262.0" layoutY="510.0" mnemonicParsing="false" onAction="#onClickExit" text="EXIT">
			<font>
				<Font name="Krungthep" size="21.0" />
			</font>
		</Button>
		<Button layoutX="50.0" layoutY="268.0" mnemonicParsing="false" onAction="#onClickSwimming" prefHeight="122.0" prefWidth="130.0" text="SWIMMING">
			<font>
				<Font name="Krungthep" size="17.0" />
			</font>
		</Button>
		<Button layoutX="235.0" layoutY="161.0" mnemonicParsing="false" onAction="#onClickCycling" prefHeight="122.0" prefWidth="130.0" text="CYCLING">
			<font>
				<Font name="Krungthep" size="17.0" />
			</font>
		</Button>
		<Button layoutX="416.0" layoutY="268.0" mnemonicParsing="false" onAction="#onClickRunning" prefHeight="122.0" prefWidth="130.0" text="RUNNING">
			<font>
				<Font name="Krungthep" size="17.0" />
			</font>
		</Button>
      <Button layoutX="257.0" layoutY="436.0" mnemonicParsing="false" onAction="#onClickBack" text="BACK">
         <font>
            <Font name="Krungthep" size="21.0" />
         </font>
      </Button>
      <ImageView fitHeight="86.0" fitWidth="226.0" layoutX="2.0" layoutY="207.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/swimming.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="115.0" fitWidth="137.0" layoutX="235.0" layoutY="290.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/cycling.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="92.0" fitWidth="137.0" layoutX="413.0" layoutY="176.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/running.png" />
         </image>
      </ImageView>
	</children>
</AnchorPane>
