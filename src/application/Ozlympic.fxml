<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600" prefWidth="600" style="-fx-background-color: #1E1F26;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AlgorithmController">
   <children>
      <Label layoutX="218.0" layoutY="43.0" text="Ozlympics" textFill="#d0e1f9">
         <font>
            <Font name="Krungthep" size="30.0" />
         </font>
      </Label>
      <Button layoutX="228.0" layoutY="146.0" mnemonicParsing="false" onAction="#onClickNewGame" text="NEW GAME">
         <font>
            <Font name="Krungthep" size="21.0" />
         </font>
      </Button>
      <Button layoutX="165.0" layoutY="249.0" mnemonicParsing="false" onAction="#onClickGameReults" text="RESULTS OF ALL GAMES">
         <font>
            <Font name="Krungthep" size="21.0" />
         </font>
      </Button>
      <Button layoutX="179.0" layoutY="359.0" mnemonicParsing="false" onAction="#onClickAthleteStandings" text=" ATHLETE STANDINGS">
         <font>
            <Font name="Krungthep" size="21.0" />
         </font>
      </Button>
      <Button layoutX="262.0" layoutY="464.0" mnemonicParsing="false" onAction="#onClickExit" text="EXIT">
     	 <font>
            <Font name="Krungthep" size="21.0" />
         </font>
      </Button>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="-24.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/Olympic-Flame.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="127.0" fitWidth="135.0" layoutX="465.0" layoutY="459.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/symbol.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
